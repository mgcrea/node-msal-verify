{
  "name": "@mgcrea/msal-verify",
  "version": "0.2.0",
  "description": "Quickly verify Microsoft Azure AD JWT tokens generated by @azure/msal-browser",
  "private": true,
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/node-msal-verify",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "msal-verify": "./dist/bin/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "cli": "tsx src/bin/cli.ts",
    "dev": "tsx watch --trace-deprecation --inspect src/index.ts",
    "dev:cli": "tsx watch --trace-deprecation --inspect src/bin/cli.ts",
    "build": "tsup",
    "preview": "node --experimental-specifier-resolution=node --no-warnings dist/index.js",
    "prettify": "prettier --write src/ test/",
    "check": "tsc --noEmit",
    "lint": "eslint src/ test/",
    "spec": "vitest --run",
    "test": "npm run lint && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@mgcrea/eslint-config-node": "^0.12.19",
    "@microsoft/api-extractor": "^7.48.0",
    "@tsconfig/node-lts": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.3",
    "@types/pem-jwk": "^2.0.2",
    "eslint": "^9.15.0",
    "jsonwebtoken": "^9.0.2",
    "pem-jwk": "^2.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.5"
  }
}
