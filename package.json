{
  "name": "@mgcrea/msal-verify",
  "version": "0.4.0",
  "description": "Quickly verify Microsoft Azure AD JWT tokens generated by @azure/msal-browser",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/node-msal-verify",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "msal-verify": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "cli": "tsx src/bin/cli.ts",
    "dev": "vitest --watch",
    "dev:cli": "tsx watch --trace-deprecation --inspect src/bin/cli.ts",
    "build": "tsup",
    "preview": "node --experimental-specifier-resolution=node --no-warnings dist/index.js",
    "prettify": "prettier --write src/ test/",
    "check": "tsc --noEmit",
    "lint": "eslint src/ test/",
    "spec": "vitest --run",
    "test": "npm run lint && npm run check && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@mgcrea/eslint-config-node": "^0.12.22",
    "@microsoft/api-extractor": "^7.50.1",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/pem-jwk": "^2.0.2",
    "eslint": "^9.21.0",
    "jsonwebtoken": "^9.0.2",
    "pem-jwk": "^2.0.0",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  }
}
